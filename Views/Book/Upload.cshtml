  @model bookallocationsystem.Models.Books.BookViewUpload
@{
    ViewData["Title"] = "Upload Books";
}
 <div class="page-breadcrumb bg-white">
                <div class="row align-items-center">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title text-uppercase font-medium font-14">Upload Books</h4>
                    </div>

                </div>
                <!-- /.col-lg-12 -->
            </div>

              <div class="container-fluid">
                     <div class="col-lg-12 col-xlg-9 col-md-12">
                        <div class="card">
                            <div class="card-body">

                                   <form  enctype="multipart/form-data"  asp-action="Upload" asp-controller="Book" method="POST">
                                           <div asp-validation-summary="All" class="text-danger"></div>
        
                                              <div class="form-group mb-4">
                                        <label class="col-sm-12">Upload</label>
                                        <div class="col-sm-12 border-bottom p-0">
                                            <input asp-for="FileUpload" type="file">


                                        </div>

                                    </div>





                                    <div class="form-group mb-4">
                                        <div class="col-sm-12">
                                            <button class="btn btn-success">Add Book</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                  </div>
